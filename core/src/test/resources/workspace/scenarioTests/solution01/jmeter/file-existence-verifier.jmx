          <stringProp name="BeanShellSampler.query">import java.nio.file.Paths;
import java.nio.file.Path;
import java.nio.file.Files;
import java.nio.file.LinkOption;

log.info(&quot;*********************************&quot;);
String workspace = props.get(&quot;workspace&quot;);
String workingDir = java.nio.file.Paths.get(&quot;.&quot;, new String[0]).normalize().toAbsolutePath().toString();
log.info(&quot;My workspace: &quot; + workspace);
log.info(&quot;Current Working Dir: &quot; + workingDir);

log.info(&quot;Checking the existence of my-infrastructure/my-deployment.txt in workspace&quot;);
Path path = Paths.get(workspace, new String[]{ &quot;my-infrastructure&quot;, &quot;my-deployment.txt&quot;});

if (!Files.exists(path, new LinkOption[0])) {
	SampleResult.setSuccessful(false);
	SampleResult.setResponseMessage(&quot;File expected to be found in: &quot; + path.toString());
	SampleResult.setResponseData(&quot;&quot;);
	IsSuccess=false;
	SampleResult.setStopTestNow(true);
}
</stringProp>
